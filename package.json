{
  "name": "yt2mp3",
  "version": "0.2.9",
  "description": "Youtube mp3 converter",
  "engines": {
    "node": ">=12.x"
  },
  "main": "index.js",
  "scripts": {
    "dev:install": "npm i gulp-cli typescript nodemon -g -f",
    "start": "node index.js",
    "dev:run": "nodemon index.js",
    "test": "node test.js",
    "update": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.gitattributes > .gitattributes && npx npm-check-updates -u"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dimaslanjaka/yt2mp3"
  },
  "keywords": [
    "node",
    "heroku",
    "express",
    "youtube",
    "mp3",
    "search"
  ],
  "license": "MIT",
  "dependencies": {
    "axios": "^1.3.5",
    "ffmpeg": "^0.0.4",
    "ffmpeg-on-progress": "^1.0.0",
    "fluent-ffmpeg": "^2.1.2",
    "moment": "^2.29.4",
    "node-localstorage": "^2.2.1",
    "sbg-utility": "^1.0.8",
    "ytdl-core": "^4.11.4"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.21",
    "@types/node": "^18.15.11",
    "@types/node-localstorage": "^1.3.0",
    "dotenv": "^16.0.3",
    "npm-check-updates": "^16.10.8"
  },
  "packageManager": "yarn@3.5.0"
